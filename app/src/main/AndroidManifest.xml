<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bijlicut2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.bijlicut2.src.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.bijlicut2.src.SignUpActivity"
            android:label="Sign Up" >
        </activity>
        <activity
            android:name="com.example.bijlicut2.src.NavigationDrawerHomePage"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name="com.example.bijlicut2.src.SearcheableActivity"
            android:label="@string/title_activity_searcheable" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data android:name="android.app.searchable"
                android:resource="@xml/searcheable"/>

        </activity>
    </application>

</manifest>
